<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.zerock.mapper.Ci_sessionMapper">

	<select id="select" resultType="ci_sessionsVO">
		select * from ci_sessions
		where
		timestamp = #{timestamp}
	</select>

	<insert id="insert">
		insert into ci_sessions
		(id,ip_address,timestamp,data)
		values
		(#{id},#{ip_address},#{timestamp},#{data})
	</insert>

	<update id="update">
		update ci_sessions set		
		id = #{id},
		ip_address = #{ip_address},
		data = #{data}
		where
		timestamp = #{timestamp}
	</update>

	<delete id="delete">
		delete from ci_sessions where timestamp = #{timestamp}
	</delete>

	<select id="listAll" resultType="ci_sessionsVO">
 		<![CDATA[ select * from ci_sessions]]>
	</select>

</mapper>